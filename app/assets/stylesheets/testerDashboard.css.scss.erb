#tests-taken-wrapper { float: right; }
.tester-test { 
	position: relative; 
	font-size: 35px; 
	width: 25%; 
	float: left; 
	margin-right: 8%; 
	margin-top: 40px;
	span { 
		width: 95%; display: inline-block; 
		padding: 5%;
	}
	.test-title { 
		background-size: 300px;	
		height:50px;
		background-color:#ddd;
 	}

	.test-link { background: #eee;		 	
		margin-top: -10px;
		display: block;
		h4 {
			font-weight:300;
			margin:10px 0px 0px;
			line-height: 1em;
			height: 35px;
			overflow: hidden;
		}
		p {
			font-size:14px;
			line-height:1.25;
			height: 50px;
			overflow: hidden;
			padding:10px 0px
		}
		button {
			width:98%;
			box-shadow:none;
			margin-top:20px;
			margin-bottom:20px;
			a {color:#fff;text-decoration:none;}
		}

	 }
	 .test-payout {color:#fff;background:#999;font-size:20px;text-align:center;

	 }
	}